<Window x:Class="AutoGestion.Vista.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controles="clr-namespace:AutoGestion.Vista"
        Title="AutoGestión" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <StackPanel DockPanel.Dock="Left" Width="200" Background="LightGray">
                <TextBlock Text="Menú" FontWeight="Bold" Margin="10"/>
                <Button Content="Registrar Cliente" Margin="10" Click="AbrirRegistrarCliente"/>
                <Button Content="Buscar Vehículo" Margin="10" Click="AbrirBuscarVehiculo"/>

            </StackPanel>

            <Frame x:Name="framePrincipal" NavigationUIVisibility="Hidden"/>
        </DockPanel>
    </Grid>
</Window>
